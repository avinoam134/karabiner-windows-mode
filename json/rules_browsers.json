{
   "rules": [
      {
         "description": "H (Ctrl) [Only Web Browsers]",
         "manipulators": [
            {
               "conditions": [
                  {
                     "bundle_identifiers": [
                        "^com\\.google\\.chrome$",
                        "^com\\.google\\.Chrome$",
                        "^org\\.mozilla\\.firefox$",
                        "^org\\.mozilla\\.nightly$",
                        "^com\\.brave\\.Browser$",
                        "^com\\.apple\\.Safari$"
                     ],
                     "type": "frontmost_application_if"
                  }
               ],
               "from": {
                  "key_code": "h",
                  "modifiers": {
                     "mandatory": [
                        "control"
                     ],
                     "optional": [
                        "any"
                     ]
                  }
               },
               "to": [
                  {
                     "key_code": "y",
                     "modifiers": [
                        "command"
                     ]
                  }
               ],
               "type": "basic"
            }
         ]
      },
