{
   "rules": [
      {
         "description": "C (Ctrl+Shift) [Only Terminal Emulators]",
         "manipulators": [
            {
               "conditions": [
                  {
                     "bundle_identifiers": [
                        "^com\\.alacritty$",
                        "^io\\.alacritty$",
                        "^co\\.zeit\\.hyper$",
                        "^com\\.googlecode\\.iterm2$",
                        "^com\\.apple\\.Terminal$",
                        "^com\\.github\\.wez\\.wezterm$",
                        "^com\\.mitchellh\\.ghostty$",
                        "^net\\.kovidgoyal\\.kitty$"
                     ],
                     "type": "frontmost_application_if"
                  }
               ],
               "from": {
                  "key_code": "c",
                  "modifiers": {
                     "mandatory": [
                        "control",
                        "shift"
                     ],
                     "optional": [
                        "any"
                     ]
                  }
               },
               "to": [
                  {
                     "key_code": "c",
                     "modifiers": [
                        "command"
                     ]
                  }
               ],
               "type": "basic"
            }
         ]
      },
